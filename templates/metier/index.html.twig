{% extends 'base.html.twig' %}

{% block title %}Metier index{% endblock %}

{% block body %}
    <h1>Metier</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Code</th>
                <th>Libmetier</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for metier in metiers %}
            <tr>
                <td>{{ metier.codeprofession }}</td>
                <td>{{ metier.libmetier }}</td>
                <td>
                    <a href="{{ path('metier_show', {'id': metier.id}) }}" class="btn btn-sm btn-primary"><i class="fas fa-eye"></i></a>
                    <a href="{{ path('metier_edit', {'id': metier.id}) }}" class="btn btn-sm btn-success"><i class="fas fa-edit"></i></a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('metier_new') }}">Create new</a>
{% endblock %}
